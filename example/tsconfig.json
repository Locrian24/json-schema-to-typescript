{
  "compilerOptions": {
    "declaration": false,
    "lib": [
    "es2020",
    "dom",
    "dom.iterable"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": ".",
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es2016",
    "types": ["node"]
  },
  "files": [
    "./index.ts"
  ]
}